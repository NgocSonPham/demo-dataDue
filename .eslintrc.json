{
  "parser": "@typescript-eslint/parser",
  "plugins": ["react"],
  "extends": ["plugin:react/recommended"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-key": "warn",
    "react/no-unescaped-entities": "warn",
    "no-console": "warn",
    "indent": ["off"]
  },
  "ignorePatterns": []
}
